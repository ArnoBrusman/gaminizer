{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "rcrowe/twigbridge": "^0.9.2",
        "barryvdh/laravel-debugbar": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "mccool/laravel-auto-presenter": "^4.0",
        "ytake/laravel-smarty": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Gaminizer\\": "app/Gaminizer",
            "Gaminizer\\Presenters\\": "app/Gaminizer/Presenters",
            "Gaminizer\\ViewComposers\\": "app/Http/ViewComposers",
            "Gaminizer\\Providers\\": "app/Gaminizer/Providers",
            "Gaminizer\\Collections\\": "app/Gaminizer/Collections",
            "App\\Http\\Controllers\\RestApi\\": "app/Http/Controllers/RestApi"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "D:/xampp/php/php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "D:/xampp/php/php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "D:/xampp/php/php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "D:/xampp/php/php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
