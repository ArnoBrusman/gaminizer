{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.7",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "rcrowe/twigbridge": "^0.9.2",
        "predis/predis": "^1.1",
        "sofa/eloquence": "~5.4",
        "qintuap/repositories": "dev-dev as 1.1.x-dev"
    },
    "require-dev": {
        "laravel/browser-kit-testing": "^2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-ide-helper": "^2.2",
        "ytake/laravel-smarty": "~2.0",
        "rap2hpoutre/laravel-log-viewer": "^0.10.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "app/",
            "Gaminizer\\": "app/Gaminizer",
            "Gaminizer\\ViewComposers\\": "app/Http/ViewComposers"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:/ArnoBrusman/qintuap-repositories.git"
        },
        {
            "type": "git",
            "url": "git@github.com:/ArnoBrusman/qintuap-scopes.git"
        },
        {
            "type": "git",
            "url": "git@github.com:/ArnoBrusman/qintuap-cachedecorators.git"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
